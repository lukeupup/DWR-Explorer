{
  "manifest_version": 3,
  "name": "DWR Explorer",
  "version": "1.1.5",
  "icons": {
    "200": "assets/dwr-logo.png"
  },
  "devtools_page": "devtools.html",
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "sandbox": {
     "pages": ["sandbox.html"]
  },
  "web_accessible_resources": [
    {
      "resources": [
         "assets/*"
      ],
      "matches": [
        "*://*/*"
      ]
    }
   
  ]
}